
class sistema:
    def __init__(self):
        self.maquina=[]
        self.pieza=[]
        self.cliente=[]
        self.pedido=[]

    def agregarCliente(self,cliente):
        self.cliente.append(cliente)
    
    def agregarMaquina(self,maquina):
        self.maquina.append(maquina)
    
    def agregarPieza(self,pieza):
        self.pieza.append(pieza)
    
    def agregarPedido(self,pedido):
        self.pedido.append(pedido)
        
